  dianji[0].yudingweizhi = 32000;//设置位置

  while (1)
  {
	  jici++;
	  if(jici == 100)
	  {
		  //HAL_TIM_PWM_Stop(&htim4,TIM_CHANNEL_1);//关闭定时�????
		  dianji[0].yudingweizhi = 0*10;//设置位置
	  }
	  if(jici == 200)
	  {
		  jici = 0;
		  //HAL_TIM_PWM_Stop(&htim4,TIM_CHANNEL_1);//关闭定时�????
		  dianji[0].yudingweizhi = 3200*10;//设置位置
	  }


//
	  HAL_Delay(20);
//	  chuankoufasong(201,dianji[0].dangqianweizhi);
//	  chuankoufasong(202,dianji[0].dangqiansudu);
//	  chuankoufasong(203,dianji[0].jiluweizhi);
////
//	  chuankoufasong(204,dianji[0].zuidasudu);
//	  chuankoufasong(205,TIM4->CNT);
////
//	  chuankoufasong(207,jjj[0]);
//	  chuankoufasong(208,jjj[1]);
//	  chuankoufasong(209,jjj[3]);
//	  chuankoufasong(210,jjj[4]);
//
	 // chuankoufasong(215,jici);
//	  chuankoufasong(216,jiasuliebiao[jici]);

